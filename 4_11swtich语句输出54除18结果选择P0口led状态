# include <reg51.h>


int main(){
    unsigned char a = 54, b = 18;
    switch (a / b)

    {
    case 1:
        P0 = 0xfe;
        break;
    case 2:
        P0 = 0xfd;
        break;
    case 3:
        P0 = 0xfb;
        break;

    case 4:
        P0 = 0xf7;
        break;
    case 5:
        P0 = 0xef;
        break;
    case 6:
        P0 = 0xdf;
        break;  
    case 7:
        P0 = 0xbf;
        break;  
    case 8: 
        P0 = 0x7f;
        break;

    default:

        P0 = 0xff;
        break;
        

    }
}
